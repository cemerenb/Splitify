    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "cemerenb.splitify.com"
    },
    "web": {
      "favicon": "../assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8ccc93fa-f98b-4363-96d6-262e4803c934"
      }
    }
  }
}
